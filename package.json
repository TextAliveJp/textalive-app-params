{
  "name": "textalive-app-params",
  "private": true,
  "version": "1.0.0",
  "description": "React を使用し、TextAlive ホストがあればフォントや配色などを調整可能な TextAlive App API の作例",
  "scripts": {
    "dev": "parcel src/index.html --dist-dir dev",
    "build": "parcel build src/index.html --public-url ./ --dist-dir docs --no-source-maps",
    "clean": "del-cli .parcel-cache dev docs",
    "prebuild": "del-cli docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TextAliveJp/textalive-app-params.git"
  },
  "keywords": [
    "textalive"
  ],
  "author": "Jun Kato <jun.kato@aist.go.jp> (https://junkato.jp)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TextAliveJp/textalive-app-params/issues"
  },
  "homepage": "https://github.com/TextAliveJp/textalive-app-params#readme",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.2.0",
    "textalive-app-api": "^0.3.0",
    "textalive-react-api": "^0.3.1"
  },
  "devDependencies": {
    "del-cli": "^3.0.1",
    "parcel": "^2.0.0-beta.3.1"
  }
}
